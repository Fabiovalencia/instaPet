<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="InstaPet.SignUpPage">

    <StackLayout>
        <Entry Placeholder="Nombre de usuario" x:Name="UsernameEntry" />
        <Entry Placeholder="Contraseña" IsPassword="True" x:Name="PasswordEntry" />
        <Button Text="Registrarse" Clicked="OnSignUpButtonClicked" />
    </StackLayout>

</ContentPage>
